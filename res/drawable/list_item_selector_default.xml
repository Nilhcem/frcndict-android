<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape android:shape="rectangle">
			<gradient
				android:angle="270"
				android:startColor="@color/divider_default_side"
				android:centerColor="@color/divider_default_middle"
				android:endColor="@color/divider_default_side" />
		</shape>
	</item>
	<item android:state_focused="true">
		<shape android:shape="rectangle">
			<gradient
				android:angle="270"
				android:startColor="@color/divider_default_side"
				android:centerColor="@color/divider_default_side"
				android:endColor="@color/divider_default_side" />
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="@color/transparent" />
		</shape>
	</item>
</selector>
