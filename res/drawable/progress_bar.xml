<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="@dimen/import_progress_radius" />
			<gradient
				android:startColor="@color/progress_bg_start"
				android:centerColor="@color/progress_bg_center"
				android:centerY="0.75"
				android:endColor="@color/progress_bg_end"
				android:angle="270" />
		</shape>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="@dimen/import_progress_radius" />
				<gradient
					android:startColor="@color/progress_fg_start"
					android:centerColor="@color/progress_fg_center"
					android:centerY="0.75"
					android:endColor="@color/progress_fg_end"
					android:angle="270" />
			</shape>
		</clip>
	</item>
</layer-list>
